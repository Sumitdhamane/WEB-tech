  <h2>Table with Colspan</h2>

  <table border="1">
    <tr>
      <td>A</td>
      <td>B</td>
      <td colspan="2">C</td>
    </tr>
    <tr>
      <td>D</td>
      <td>E</td>
      <td>F</td>
      <td>G</td>
    </tr>
    <tr>
      <td>H</td>
      <td colspan="3">I</td>
    </tr>
  </table>

